        "#Funcion #3\n",
        "\n",
        "def contardigit(n):\n",
        "    count = 0\n",
        "    while n != 0:\n",
        "      n //=10\n",
        "      count += 1\n",
        "    return count\n",
        "  \n",
        "n = int(input(\"Digite el numero:\"))\n",
        "print(\"Numero digitados: % d\" % (contardigit(n)))"
